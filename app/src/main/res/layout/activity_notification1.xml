<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/notification1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black">

    <TextView
        android:id="@+id/새로운알림생기면"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="새로운 알림이 생기면 알려드릴게요."
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Newkey123456" />

    <ImageView
        android:id="@+id/Newkey123456"
        android:layout_width="140dp"
        android:layout_height="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/newkey12346" />

    <FrameLayout
        android:id="@+id/frame_81"
        android:layout_width="409dp"
        android:layout_height="80dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/button_back"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="29dp"
            android:background="@android:color/transparent"
            android:contentDescription="뒤로가는버튼"
            android:padding="10dp"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/button_back"
            tools:ignore="SpeakableTextPresentCheck"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/알림"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="64dp"
            android:layout_marginTop="39dp"
            android:fontFamily="@font/pretendardmedium"
            android:gravity="center_vertical"
            android:includeFontPadding="false"
            android:text="알림"
            android:textColor="@color/white"
            android:textSize="19dp" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>